<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.hsd.core.board.repository.FAQCategoryRepository">
    <resultMap id="FAQCategoryResultMap" type="FAQCategory">
        <id		property="idx" column="IDX" />
        <result property="category_code" column="CATEGORY_CODE" />
        <result property="category_name" column="CATEGORY_NAME" />

    </resultMap>

    <!-- faq 분류 목록 -->
    <select id="getFAQCategory" resultMap="FAQCategoryResultMap">
        select * from TB_FAQ_CATEGORY
    </select>

</mapper>