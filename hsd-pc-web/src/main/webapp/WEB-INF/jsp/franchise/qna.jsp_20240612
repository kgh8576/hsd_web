                                    </dl>
                                </div>
                                <!-- //이름 -->
                                <!-- 이메일 -->
                                <div class="in_box">
                                    <dl class="in_box_th">
                                        <dt>이메일</dt>
                                        <dd>
                                        <span class="form text">
                                            <input type="text" id="fck_email" name="email" style="ime-mode:active">
                                            <label for="fck_email">이메일 주소를 입력해주세요</label>
                                        </span>
                                        </dd>
                                    </dl>
                                </div>
                                <!-- //이메일 -->
