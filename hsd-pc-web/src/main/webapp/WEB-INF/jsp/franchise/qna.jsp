<body>

<div class="content franchise">
    <section class="qna mo_version">
        <!-- 창업문의 -->
        <div class="customer_wrap tab_st01">
            <!-- title -->
            <div class="title">
                <h2 class="h2_tit h2_01">창업 문의</h2>
            </div>
            <!-- //title -->

            <div class="customer_cont cont_st02">
                <!-- 창업문의_left -->
                <div class="customer_tit fz_02">
                    <p>
                        <span class="s_block">궁금하신 점을</span>
                        <span class="s_block">문의하시면</span>
                        <span class="s_block">창업상담 전문가가</span>
                        <span class="s_block">빠르게 답변 드립니다</span>
                    </p>
                </div>
                <!-- //창업문의_left -->

                <!-- 창업문의_right -->
                <form:form id="regForm" name="regForm" autocomplete="false" onsubmit="return false;">
                    <div class="customer_sub">
                        <div class="customer_info pa_02">
                            <!-- 담당자 연락처 -->
                            <div class="customer_form">
                                <h4 class="h4_tit fz_02">창업상담 접수 전화번호 안내</h4>
                                <!-- 담당자 연락처 table -->
                                <div class="call_tb">
                                    <table>
                                        <colgroup>
                                            <col class="ca_col_01"/>
                                            <col class="ca_col_02"/>
                                        </colgroup>
                                        <thead>
                                        <tr>
                                            <th><span style='font-size:20px;'>전화번호</span></th>
                                            <th><span style='font-size:20px;'>운영시간</span></th>
                                        </tr>
                                        </thead>
										<tbody>
                                        
										<tr>
                                            <td>
                                                <div class="area"><span style='font-size:18px;'>070-8095-6020</span></div>
                                            </td>
                                            <td rowspan="1">
                                                <span style='font-size:18px; line-height: normal;'>
													주 5일 / 법정공휴일 휴무<br>
													월~금 : 09시 ~ 18시<br>
													(12시 ~ 13시 점심시간 제외)<br>
												</span>
                                            </td>
										</tr>
											
                                        </tbody>
                                    </table>
                                </div>
                                <!-- //담당자 연락처 table -->
                            </div>
                            <!-- //담당자 연락처 -->

                            <!-- 고객정보 -->
                            <div class="customer_form form_02">
                                <h4 class="h4_tit fz_02">온라인 문의하기</h4>
                                <!-- 이름 -->
                                <div class="in_box">
                                    <dl class="in_box_th">
                                        <dt>이름</dt>
                                        <dd>
                                        <span class="form text">
                                            <input type="text" id="fck_text_01" name="name">
                                            <label for="fck_text_01">이름을 입력해주세요</label>
                                        </span>
                                        </dd>
                                    </dl>
                                </div>
                                <!-- //이름 -->
                                <!-- 이메일 -->
                                <div class="in_box">
                                    <dl class="in_box_th">
                                        <dt>이메일</dt>
                                        <dd>
                                        <span class="form text">
                                            <input type="text" id="fck_email" name="email" style="ime-mode:active">
                                            <label for="fck_email">이메일 주소를 입력해주세요</label>
                                        </span>
                                        </dd>
                                    </dl>
                                </div>
                                <!-- //이메일 -->
                                <!-- 휴대폰 -->
                                <div class="in_box">
                                    <dl class="in_box_th">
                                        <dt>휴대폰</dt>
                                        <dd>
                                        <span class="form text">
                                            <input type="text" id="fck_phone" name="hp">
                                            <label for="fck_phone">휴대폰 번호를 입력해주세요</label>
                                        </span>
                                        </dd>
                                    </dl>
                                </div>
                                <!-- //휴대폰 -->
                            </div>
                            <!-- //고객정보 -->
                            <!-- 창업문의 -->
                            <div class="customer_form">
                                <!-- 가맹 예정지 -->
                                <div class="in_box first">
                                    <dl class="in_box_th">
                                        <dt>가맹 예정지</dt>
                                        <!-- 국내/해외 선택 -->
                                        <dd class="loctype_select">
                                        <span class="form radio">
                                            <label>
                                                <input type="radio" name="storeLoc" value="D" checked="checked">
                                                <span>국내</span>
                                            </label>
					<!--
                                            <label>
                                                <input type="radio" name="storeLoc" value="A" onclick="return false;" >
                                                <span>해외</span>
                                            </label>
					-->
                                        </span>
                                        </dd>
                                        <!-- //국내/해외 선택 -->
                                        <!-- 주소 -->
                                        <dd class="shop_select address_wrap">
                                            <!-- 주소찾기 -->
                                            <span class="form text">
                                            <input type="text" id="zipcode" name="zipcode" readonly="readonly"
                                                   maxlength="5" onclick="searchAddress();">
                                            <label for="zipcode"></label>
                                            <a href="javascript:searchAddress();" class="btn_overlap">주소찾기</a>
                                        </span>
                                            <!-- //주소찾기 -->
                                            <!-- 주소 -->
                                            <span class="form text ad_se">
                                            <input type="text" id="addr1" name="addr1" readonly="readonly">
                                            <label for="addr1">주소 입력</label>
                                        </span>
                                            <!-- //주소 -->
                                            <!-- 상세주소 -->
                                            <span class="form text address">
                                            <input type="text" id="addr2" name="addr2">
                                            <label for="addr2">상세주소 입력</label>
                                        </span>
                                            <!-- //상세주소 -->
                                        </dd>
                                    </dl>
                                </div>
                                <!-- //가맹 예정지 -->
                                <!-- 점포 유무 -->
                                <div class="in_box">
                                    <dl class="in_box_th">
                                        <dt>점포 유무</dt>
                                        <dd class="store_select">
                                            <div class="radio_wrap">
                                            <span class="form radio">
                                                <label>
                                                    <input type="radio" name="storeYn" value="Y" checked="checked">
                                                    <span>유</span>
                                                </label>
                                            </span>
                                                <!-- 평수 -->
                                                <span class="form text store_input">
                                                <input type="text" id="storeSpace" name="storeSpace" value="">
                                                <label for="storeSpace">평수입력</label>
                                                <em class="btn_overlap line_no">평</em>
                                            </span>
                                                <!-- //평수 -->
                                                <span class="form radio">
                                                <label>
                                                    <input type="radio" name="storeYn" value="N">
                                                    <span>무</span>
                                                </label>
                                            </span>
                                            </div>
                                        </dd>
                                    </dl>
                                </div>
                                <!-- //점포 유무 -->
                                <!-- 창업시기 -->
                                <div class="in_box">
                                    <dl class="in_box_th">
                                        <dt>창업시기</dt>
                                        <dd>
                                        <span class="select">
                                            <select class="classic" title="창업시기" id="foundationTime"
                                                    name="foundationTime">
                                                <option value="">선택해주세요.</option>
                                            </select>
                                        </span>
                                        </dd>
                                    </dl>
                                </div>
                                <!-- //창업시기 -->
                                <!-- 예상창업비용 -->
                                <div class="in_box">
                                    <dl class="in_box_th">
                                        <dt>예상창업비용</dt>
                                        <dd class="cost_wrap">
                                        <span class="select">
                                            <select class="classic" title="예상창업비용" id="foundationCost"
                                                    name="foundationCost">
                                                <option value="">선택해주세요.</option>
                                            </select>
                                        </span>
                                            <p class="cost">※ 점포 비용 포함</p>
                                        </dd>
                                    </dl>
                                </div>
                                <!-- //예상창업비용 -->
                                <!-- 한솥 선택 경로 -->
                                <div class="in_box">
                                    <dl class="in_box_th">
                                        <dt>한솥 선택 경로</dt>
                                        <dd>
                                        <span class="select">
                                            <select class="classic" title="한솥 선택 경로" id="selectType" name="selectType">
                                                <option value="">선택해주세요.</option>
                                            </select>
                                        </span>
                                        </dd>
                                    </dl>
                                </div>
                                <!-- //한솥 선택 경로 -->
                                <!-- 문의 내용 -->
                                <div class="in_box in_qna">
                                    <dl class="in_box_th">
                                        <dt>문의 내용</dt>
                                        <dd>
                                        <span class="form text">
                                            <input type="text" id="title" name="title">
                                            <label for="title">제목을 입력해주세요.</label>
                                        </span>
                                        </dd>
                                        <dd>
                                            <div class="textarea_wrap cont_area">
                                                <textarea title="내용입력" placeholder="내용을 입력해주세요."
                                                          name="contents"></textarea>
                                            </div>
                                        </dd>
                                    </dl>
                                </div>
                                <!-- //문의 내용 -->
                            </div>
                            <!-- //창업문의 -->
                            <jsp:include page="/WEB-INF/layout/common/privacy.jsp"/>
                        </div>
                        <!-- btn -->
                        <div class="customer_btn">
                                <%--<div class="btn_wrap">--%>
                                <%--<span class="btn btn_st03"><a href="#none" class="c_03" onclick="$('#regForm').reset();">취소</a></span>--%>
                                <%--</div>--%>
                            <div class="btn_wrap">
                                <span class="btn btn_st03"><a href="#none" class="c_01"
                                                              onclick="$('#regForm').submit();">보내기</a></span>
                            </div>
                        </div>
                        <!--// btn -->
                    </div>
                </form:form>
                <!-- //창업문의_right -->
            </div>

        </div>
        <!-- //창업문의 -->
    </section>
</div>

<jsp:include page="/WEB-INF/jsp/more/franchise_more.jsp"/>

<script type="text/javascript">
  $(function () {

    // 주소지선택
    $("input[name=storeLoc]").change(function () {
      if (this.value == 'D') {
        $(".address_wrap span:lt(2)").show();
        $(".address_wrap span:eq(2)").css("margin-top", "10px");
      } else {
        $(".address_wrap span:lt(2)").hide();
        $(".address_wrap span:eq(2)").css("margin-top", "0");
      }
    });

    // 점포유무
    $("input[name=storeYn]").change(function () {
      if ($("input[name=storeYn]:checked").val() == 'Y') {
        $("#storeSpace").removeAttr("disabled");
      } else {
        $("#storeSpace").prop("disabled", "disabled").val("");
      }
    });

    // 창업시기
    searchCode('FT01', 'foundationTime');

    // 예상창업비용
    searchCode('FC01', 'foundationCost');

    // 한솥 선택 경로
    searchCode('ST01', 'selectType');

    // validate & submit
    $('#regForm').validate({
      rules: {
        name: {
          required: true
        },
        email: {
          required: true,
          email: true
        },
        hp: {
          required: true,
          chkPhone: true
        },
        zipcode: {
          required: function () {
            return $("input[name=storeLoc]:checked").val() == 'D' ? true : false;
          }
        },
        addr1: {
          required: function () {
            return $("input[name=storeLoc]:checked").val() == 'D' ? true : false;
          }
        },
        addr2: {
          required: function () {
            return $("input[name=storeLoc]:checked").val() == 'D' ? false : true;
          }
        },
        storeSpace: {
          required: function () {
            return $("input[name=storeYn]:checked").val() == 'Y' ? true : false;
          },
          number: true
        },
        foundationTime: {
          required: true,
        },
        foundationCost: {
          required: true,
        },
        selectType: {
          required: true,
        },
        title: {
          required: true
        },
        contents: {
          required: true
        }
      },
      messages: {
        name: {
          required: "이름을 입력해 주세요."
        },
        email: {
          required: "이메일을 입력해 주세요.",
          email: "이메일을 정확히 입력해 주세요."
        },
        hp: {
          required: "휴대폰번호를 입력해 주세요.",
          chkPhone: "휴대폰번호를 정확히 입력해 주세요. ex)010-0000-0000"
        },
        zipcode: {
          required: "우편번호를 입력해 주세요."
        },
        addr1: {
          required: "가맹 예정지 주소를 입력해 주세요."
        },
        addr2: {
          required: "가맹 예정지 주소를 입력해 주세요."
        },
        storeSpace: {
          required: "점포 평수를 입력해 주세요.",
          number: "점포 평수는 숫자로 입력해 주세요."
        },
        foundationTime: {
          required: "창업시기를 선택해 주세요.",
        },
        foundationCost: {
          required: "예상창업비용을 선택해 주세요.",
        },
        selectType: {
          required: "한솥 선택 경로를 선택해 주세요.",
        },
        title: {
          required: "제목을 입력해 주세요."
        },
        contents: {
          required: "내용을 입력해 주세요."
        }
      },
      submitHandler: function (form) {

        if (!$("#agree_01").is(":checked")) {
          alert("개인정보의 필수적인 사항에 대한 수집이용에 동의해주세요.");
          $("#agree_01").focus();
          return;
        }

        $.ajax({
          type: 'PUT',
          url: "${cp}/api/qna",
          headers: {
            Accept: "application/json; charset=utf-8",
            "Content-Type": "application/json; charset=utf-8"
          },
          data: JSON.stringify(tss.util.serializeObject($(form)))
        }).done(function () {
          tss.util.alert("창업문의가 접수되었습니다.\n확인을 누르시면 메인화면으로 이동합니다.");
          location.href = "${cp}/";
        }).fail(function () {
          tss.util.alert("일시적인 오류가 발생했습니다.\n화면을 새로고침 한 이후에 다시 시도해 주세요.");
        });
      }
    });

  });
</script>
